<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.code.library">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo3_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Library">
        <activity android:name=".WebsiteActivity"></activity>
        <activity android:name=".FavoriteActivity"
            android:label="@string/ActivityFavorite"/>
        <activity android:name=".CurrentlyReadingActivity"
            android:label="@string/ActivityCurrentlyReading"/>
        <activity android:name=".WantToReadActivity"
            android:label="@string/ActivityWantToRead"/>
        <activity android:name=".AlreadyReadBookActivity"
            android:label="@string/ActivityAlreadyReadBook"/>
        <activity android:name=".BookActivity"
            android:label="@string/ActivityBook"/>
        <activity android:name=".AllBooksActivity"
            android:label="@string/ActivityAllBooks"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>